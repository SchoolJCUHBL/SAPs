string printCount (vector<byte> &number) //print number
{
	string output;
	for (int i = number.size, i => 0, i--)
	{
		output += number.at(i).toString();
	}
	return output;
}

vector<byte> correct (vector<byte> &number, int position = 0, int system = 10) //correct function
{
	while (number.at(position) > system-1)
	{
		number.at(position) -= system;
		try
		{
			number.at(position+1)++;
		}
		catch (...)
		{
			number.emplace_back(1);
		}
	}
	return correct(number, position+1);
}

vector<byte> add (vector<byte> number, int addition)
{
	number[0] += addition;
	return correct(number);
}
